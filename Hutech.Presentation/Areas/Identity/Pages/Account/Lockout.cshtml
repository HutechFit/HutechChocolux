@page
@model LockoutModel
@{
    ViewData["Title"] = "Locked out";
}

<section class="contact_section layout_padding">
    <div class="d-flex align-items-center justify-content-center vh-25">
        <div class="text-center">
            <h1 class="display-1 fw-bold">Error</h1>
            <p class="fs-3">
                <span class="text-danger">
                    Whoops!
                </span> 
                You are locked out.
            </p>
            <p class="lead">
                You are temporarily locked out of your account. Please try again later, or contact the administrator.
            </p>
            <a asp-area=""
               asp-page="/Index"
               class="btn btn-primary">
                Back to homepage
            </a>
        </div>
    </div>
</section>